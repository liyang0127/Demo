  <process id="leave" name="请假流程" isExecutable="true">
    <startEvent id="startevent1" name="start"></startEvent>
    <userTask id="usertask1" name="提交申请" activiti:assignee="${requestUser.phone}"></userTask>
    <userTask id="usertask2" name="领导审批" activiti:assignee="${dealUser.phone}"></userTask>
    <sequenceFlow id="flow1" name="提交" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow4" name="不提交上级" sourceRef="exclusivegateway2" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${up =='false'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="usertask2" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${transition=='审批成功'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask1" name="审批成功" activiti:delegateExpression="${updateSuccessStatusService}"></serviceTask>
    <sequenceFlow id="flow14" sourceRef="usertask2" targetRef="servicetask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${transition=='审批失败'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask2" name="审批失败" activiti:delegateExpression="${updateFailStatusService}"></serviceTask>
    <sequenceFlow id="flow16" sourceRef="exclusivegateway2" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${up=='true'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow17" sourceRef="servicetask2" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow18" sourceRef="servicetask1" targetRef="exclusivegateway2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${up =='true'}]]></conditionExpression>
    </sequenceFlow>
  </process>
